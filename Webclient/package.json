{
  "name": "webclient",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:mac": "ng serve --proxy-config proxy.conf.json --port 80",
    "start:windows": "ng serve --proxy-config proxy.conf.json --port 80",
    "prebuild": "bash tools/prepare.sh",
    "prebuild:mac": "bash tools/prepare.sh",
    "prebuild:windows": "bash tools/prepare.sh",
    "prestart": "bash tools/prepare.sh",
    "prestart:mac": "bash tools/prepare.sh",
    "prestart:windows": "bash tools/prepare.sh",
    "pretest": "bash tools/prepare.sh",
    "pretest:mac": "bash tools/prepare.sh",
    "pretest:windows": "bash tools/prepare.sh",
    "prelint": "bash tools/prepare.sh",
    "prelint:mac": "bash tools/prepare.sh",
    "prelint:windows": "bash tools/prepare.sh",
    "build": "ng build --prod --configuration=production --build-optimizer",
    "postbuild": "bash tools/minifyHtml.sh && bash tools/inlineGlobalStyles.sh",
    "test": "jest",
    "lint": "ng lint",
    "postinstall": "ngcc --tsconfig \"./tsconfig.app.json\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.0.0",
    "@angular/cdk": "^12.0.0",
    "@angular/common": "^12.0.0",
    "@angular/compiler": "^12.0.0",
    "@angular/core": "^12.0.0",
    "@angular/forms": "^12.0.0",
    "@angular/platform-browser": "^12.0.0",
    "@angular/platform-browser-dynamic": "^12.0.0",
    "@angular/router": "^12.0.0",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "chart.js": "^2.9.3",
    "chartjs-plugin-zoom": "^0.7.7",
    "comlink": "^4.3.0",
    "ng-multiselect-dropdown": "^0.2.10",
    "ng2-charts": "^2.4.2",
    "rxjs": "^6.6.6",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.0.0",
    "@angular/cli": "^12.0.0",
    "@angular/compiler-cli": "^12.0.0",
    "@angular/language-service": "^12.0.0",
    "@types/jest": "^24.0.22",
    "@types/node": "^12.12.50",
    "codelyzer": "^6.0.0",
    "html-minifier": "^4.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "jest": "^26.6.3",
    "jest-preset-angular": "^8.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "typescript": "~4.2.4"
  }
